package com.danu.example1;

import java.util.ArrayList;
import java.util.List;

public class ArrayListSample {

	public static void main(String args[]){
		
		List<Integer> list = new ArrayList<String>(100);
		
		System.out.println("ArrayList size is " + list.size());
		
		list.add("A");
		list.add("E");
		list.add("I");
		list.add("O");
		list.add("U");
		
		/*System.out.println("ArrayList size is " + list.size());
		
		list.remove(3);
		
		System.out.println("ArrayList size is " + list.size());
		
		for(String string: list){
			System.out.println(string);
		}*/
		
		List<Integer> list1 = new ArrayList<>();
		list1.add(2);
		list1.add(4);
		list1.add(6);
		list1.add(8);
		list1.add(10);
		
		
		//System.out.println("ArrayList size is " + list.removeAll(list1));
		list.removeAll(list1);
		//list.retainAll(list1);
		System.out.println("ArrayList size is " + list);
		
	}
}
