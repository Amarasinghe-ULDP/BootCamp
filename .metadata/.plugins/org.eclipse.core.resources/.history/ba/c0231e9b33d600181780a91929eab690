		HashMap<String,String> hm = new HashMap<String,String>();
		
		hm.put("Danu", "javaSE,J2EE");
		hm.put("Jony", "Java");
		hm.put("geeth", "ME");
		hm.put("Dush", "Spring");
		
		for(String key: hm.keySet()){
			System.out.println(key + " : " + hm.get(key));
		} 
		
		}
		
	}

}
